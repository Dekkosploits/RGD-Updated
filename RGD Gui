-- Script

while true do
	TextLabel_5.Text = "Destroying"
	wait(.5)
	TextLabel_5.Text = "Destroying."
	wait(.5)
	TextLabel_5.Text = "Destroying.."
	wait(.5)
	TextLabel_5.Text = "Destroying..."
	wait(.5)
end
