StopDroids.Name = "StopDroids"
StopDroids.Parent = MainFrame
StopDroids.BackgroundColor3 = Color3.fromRGB(60, 0, 182)
StopDroids.Position = UDim2.new(0.067, 0,0.483, 0)
StopDroids.Size = UDim2.new(0.866714358, 0, 0.104358107, 0)
StopDroids.Font = Enum.Font.Arcade
StopDroids.Text = "Stop Droids"
StopDroids.TextColor3 = Color3.fromRGB(255, 255, 255)
StopDroids.TextScaled = true
StopDroids.TextSize = 14.000
StopDroids.TextStrokeTransparency = 0.000
StopDroids.TextWrapped = true
StopDroids.MouseButton1Click:Connect(function()
	if StopDroids.Text == "Stop Droids" then

		setsimulationradius(math.huge,math.huge)

		while wait() do
			for i,v in pairs(game.Workspace.Room.Enemies:GetDescendants()) do
				if v:IsA("Humanoid") and v.Parent.Name ~= game.Players.LocalPlayer.Name then
					v.WalkSpeed = 0
				end
			end
		end		
	end		
end)


